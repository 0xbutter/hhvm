.main {
  DefCls 0
  FPushFuncD 0 "run"
  FCall 0
  PopR
  Int 1
  RetC
}

.function <"" N  > f(["A1"("""a:0:{}""")]$p1, ["A2"("""a:0:{}""")]$p2) {
  Null
  RetC
}

.function <"" N  > test($caption, $parameters, $i, $attribute) {
  .declvars $p;
  BaseL $parameters Warn
  QueryM 0 CGet EL:$i
  FPushObjMethodD 1 "getAttribute" NullThrows
  CGetL $attribute
  FPassCNop
  FCall 1
  UnboxR
  SetL $p
  PopC
  String ":"
  CGetL2 $caption
  Concat
  IssetL $p
  JmpZ L0
  String "SET"
  Jmp L1
L0:
  String "NOTSET"
L1:
  Concat
  Print
  PopC
  String "\\n"
  Print
  PopC
  Null
  RetC
}

.function <"" N  > run() {
  .declvars $cls $ctor $func;
  FPushCtorD 1 "ReflectionClass"
  String "A"
  FPassCNop
  FCall 1
  PopR
  SetL $cls
  PopC
  CGetL $cls
  FPushObjMethodD 0 "getConstructor" NullThrows
  FCall 0
  UnboxR
  SetL $ctor
  PopC
  FPushFuncD 4 "test"
  String "1. existing attribute"
  FPassCNop
  CGetL $ctor
  FPushObjMethodD 0 "getParameters" NullThrows
  FCall 0
  UnboxR
  FPassCNop
  Int 0
  FPassCNop
  String "Annotation"
  FPassCNop
  FCall 4
  PopR
  FPushFuncD 4 "test"
  String "2. missing attribute"
  FPassCNop
  CGetL $ctor
  FPushObjMethodD 0 "getParameters" NullThrows
  FCall 0
  UnboxR
  FPassCNop
  Int 0
  FPassCNop
  String "Annotation-missing"
  FPassCNop
  FCall 4
  PopR
  FPushCtorD 1 "ReflectionFunction"
  String "f"
  FPassCNop
  FCall 1
  PopR
  SetL $func
  PopC
  FPushFuncD 4 "test"
  String "3. existing attribute"
  FPassCNop
  CGetL $func
  FPushObjMethodD 0 "getParameters" NullThrows
  FCall 0
  UnboxR
  FPassCNop
  Int 0
  FPassCNop
  String "A1"
  FPassCNop
  FCall 4
  PopR
  FPushFuncD 4 "test"
  String "4. missing attribute"
  FPassCNop
  CGetL $func
  FPushObjMethodD 0 "getParameters" NullThrows
  FCall 0
  UnboxR
  FPassCNop
  Int 0
  FPassCNop
  String "A1-missing"
  FPassCNop
  FCall 4
  PopR
  FPushFuncD 4 "test"
  String "5. existing attribute"
  FPassCNop
  CGetL $func
  FPushObjMethodD 0 "getParameters" NullThrows
  FCall 0
  UnboxR
  FPassCNop
  Int 1
  FPassCNop
  String "A2"
  FPassCNop
  FCall 4
  PopR
  FPushFuncD 4 "test"
  String "6. missing attribute"
  FPassCNop
  CGetL $func
  FPushObjMethodD 0 "getParameters" NullThrows
  FCall 0
  UnboxR
  FPassCNop
  Int 1
  FPassCNop
  String "A2-missing"
  FPassCNop
  FCall 4
  PopR
  Null
  RetC
}

